# OpenRouter Configuration
# Copy this file to .env and fill in your API key

# OpenRouter API endpoint
ANTHROPIC_BASE_URL=https://openrouter.ai/api

# Your OpenRouter API key (get one at https://openrouter.ai/keys)
ANTHROPIC_AUTH_TOKEN=your-openrouter-api-key-here

# Must be empty when using OpenRouter
ANTHROPIC_API_KEY=

# Default model - using free GLM model
# See https://openrouter.ai/models for other options
ANTHROPIC_DEFAULT_SONNET_MODEL=z-ai/glm-4.5-air:free
